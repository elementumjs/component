{
  "name": "@elementumjs/component",
  "version": "0.0.1",
  "description": "Simple WebComponents library based on ElementumJS utils",
  "files": [
    "src/*.js",
    "dist/*.js"
  ],
  "main": "dist/component.cjs.js",
  "module": "dist/component.esm.js",
  "browser": "dist/component.umd.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "test": "jest --config=build/jest.config.js",
    "build": "NODE_ENV=production rollup -c build/rollup.config.js",
    "dev": "rollup -w -c build/rollup.config.js",
    "start": "npm run dev",
    "docs": "typedoc --tsconfig build/tsconfig.json --mode file --out docs --hideSources --hideGenerator --hideBreadcrumbs src/lib/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementumjs/component.git"
  },
  "author": "Lucas Menendez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elementumjs/component/issues"
  },
  "homepage": "https://github.com/elementumjs/component#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "^7.13.5",
    "@babel/runtime": "^7.13.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "babel-jest": "^26.6.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^26.6.3",
    "rollup": "^2.39.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.2",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.28",
    "typedoc-plugin-markdown": "^3.5.0",
    "typedoc-plugin-no-inherit": "^1.2.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@elementumjs/listenable-data": "^0.1.9",
    "@elementumjs/template": "^0.1.10"
  }
}
